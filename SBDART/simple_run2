#!/bin/bash

root=sbchk_transmittance_and_height

# Remove existing output file
rm -f $root.transmittance_and_height

# Run SBDART with the modified input file
echo "Running SBDART for transmittance and height..."
./sbdart > $root.transmittance_and_height < INPUT

echo "SBDART run completed. Output saved to $root.transmittance_and_height"
